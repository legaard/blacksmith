{
  "name": "fizzy",
  "version": "0.1.0",
  "description": "Supercharged fixture library for organizing and constructing test data",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint && npm run test",
    "build": " tsc -p tsconfig.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "author": "Lasse Legaard",
  "license": "MIT",
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/uuid": "^3.4.4",
    "babel-jest": "^23.6.0",
    "jest": "^22.4.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "keywords": [
    "test",
    "testing",
    "unit",
    "integration",
    "fixture",
    "autofixture",
    "factory",
    "builder",
    "tdd",
    "automation",
    "typescript",
    "javascript"
  ],
  "homepage": "https://github.com/legaard/fizzy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/legaard/fizzy.git"
  },
  "bugs": {
    "url": "https://github.com/legaard/fizzy/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
